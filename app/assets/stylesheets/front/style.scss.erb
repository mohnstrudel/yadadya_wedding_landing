.g-main{
	background-color: rgba(50, 55, 53, 0.7);
	background-image: url(<%= asset_path 'landing/top-bg.jpg' %>);
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	min-height: calc(400rem / 13);
	height: 100vh;
	position: relative;
	padding-top: 0;
	overflow: hidden;
}
.g-main:after{
	content: "";
	display: block;
	z-index: 1;
	width: 36%;
	height: 100%;
	position: absolute;
	right: 0;
	top: 0;
	background: url(<%= asset_path 'landing/top-bg-icons.svg' %>);
	background-position: center left;
	background-repeat: no-repeat;
	background-size: cover;
	transition: transform 500ms 300ms linear;
	transform: translateX(100vw);
}
.g-main:before{
	content: "";
	display: block;
	background: url(<%= asset_path 'landing/top-bg-waves.svg' %>);
	background-position: center right;
	background-repeat: no-repeat;
	background-size: cover;
	z-index: 0;
	width: 69%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	transform: translateX(-100vw);
	transition: transform 500ms 600ms linear;
}
@media screen and (orientation: portrait){
	.g-main{
		max-height: calc(900rem / 13);
	}
}
